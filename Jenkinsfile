pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World! This is Krushant'
            }
        }
        stage('Stage 2') {
            steps {
                echo 'welcome to stage 2'
            }
        }
        stage('stage 3') {
            steps {
                echo 'hi'
            }
        }
    }
}
